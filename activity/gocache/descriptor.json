{
    "name": "gocache",
    "type": "flogo:activity",
    "version": "1.0.0",
    "title": "Gocache",
    "description": "An in-memory key:value store",
    "homepage": "https://github.com/ryan-2048/flogo-components/tree/master/activity/gocache",
    "inputs": [
        {
            "name": "action",
            "type": "string",
            "allowed": [
                "SET",
                "OBJ_SET",
                "GET",
                "DELETE"
            ],
            "description": "action"
        },
        {
            "name": "key",
            "type": "string",
            "description": "key"
        },
        {
            "name": "value",
            "type": "object",
            "description": "value"
        },
        {
            "name": "expiryTime",
            "type": "int",
            "description": "expiryTime"
        },
        {
            "name": "purgeTime",
            "type": "int",
            "description": "purgeTime"
        }
    ],
    "outputs": [
        {
            "name": "error",
            "type": "bool",
            "description": "Flag indicating if there was an error executing the script"
          },
          {
            "name": "errorMessage",
            "type": "string",
            "description": "The error message"
          },
          {
            "name": "result",
            "type": "object",
            "description": "The result object from the javascript code"
          }
    ]
}
